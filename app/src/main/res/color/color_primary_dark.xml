<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- State when the view is pressed -->
    <item android:state_pressed="true" android:color="@color/colorPrimaryDarkPressed" />

    <!-- State when the view is focused -->
    <item android:state_focused="true" android:color="@color/colorPrimaryDarkFocused" />

    <!-- Default state -->
    <item android:color="@color/colorPrimaryDark" />

</selector>
